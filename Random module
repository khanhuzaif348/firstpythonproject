#choioce : usage  = pick a random no. from list
from random import choice
l1 = [2,4,5,6,5]
print("your choice : ",choice(l1))

#shuffle : usage =  shuffle(Exchange) a value in list
from random import shuffle
l2= ["apppl","basnana","saaskma"]
shuffle(l2)
print("your shuffle listed :",l2)



#randint  : usage  = pick a random value to give a distance number
from random import randint
a = randint(100,999)
print("your otp : ",a)
